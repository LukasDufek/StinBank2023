    "axios-mock-adapter": "^1.21.4",
    "body-parser": "^1.20.2",
    "concurrently": "^8.0.1",
    "cors": "^2.8.5",
    "coverage": "^0.4.1",
    "ejs": "^3.1.9",
    "emailjs-com": "^3.2.0",
    "express": "^4.18.2",
    "fs": "^0.0.1-security",
    "jest": "^29.5.0",
    "localStorage": "^1.0.4",
    "mockingoose": "^2.16.2",
    "mongoose": "^7.1.0",
    "nodemailer": "^6.9.1",
    "raw-loader": "^4.0.2",
    "supertest": "^6.3.3",
    "vuex": "^4.1.0"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.21.4",
    "@babel/preset-env": "^7.21.5"
  },
  "jest": {
